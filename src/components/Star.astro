---
import type { HTMLAttributes } from "astro/types";

export interface Props extends HTMLAttributes<"svg"> {
    viewBox?: string;
    title?: string;
    children?: any;
}

const { viewBox = "0 -960 960 960", title = "Star" } = Astro.props;
---

<svg
    role="img"
    aria-hidden="true"
    viewBox={viewBox}
    width="24"
    height="24"
    shape-rendering="auto"
    style="margin-right: 0.5em;"
>
    <title>{title}</title>
    <path
        fill="currentColor"
        d="m323-205 157-94 157 95-42-178 138-120-182-16-71-168-71 167-182 16 138 120-42 178ZM233-80l65-281L80-550l288-25 112-265 112 265 288 25-218 189 65 281-247-149L233-80Zm247-355Z"
    ></path>
</svg>
